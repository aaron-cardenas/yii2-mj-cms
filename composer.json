{
  "name": "mobilejazz/yii2-mj-cms",
  "description": "Yii 2 CMS Module developed by MobileJazz",
  "version": "dev-develop",
  "keywords": [
    "yii2",
    "extension",
    "cms"
  ],
  "homepage": "https://github.com/mobilejazz/yii2-mj-cms",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/mobilejazz/yii2-mj-cms/issues?state=open",
    "source": "https://github.com/mobilejazz/yii2-mj-cms"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "yiisoft/yii2": "2.0.*",
    "dmstr/yii2-bootstrap": "*",
    "yiisoft/yii2-bootstrap": "2.0.*",
    "yiisoft/yii2-swiftmailer": "2.0.*",
    "yiisoft/yii2-jui": "2.0.*",
    "yiisoft/yii2-imagine": "2.0.*",
    "fishvision/yii2-migrate": "1.0.*",
    "trntv/yii2-datetime-widget": "1.0.*",
    "trntv/yii2-aceeditor": "2.0.*",
    "trntv/probe": "0.2.*",
    "phpoffice/phpexcel": "*",
    "mobilejazz/yii2-oauth2-server": "2.0.*",
    "mobilejazz/yii2-export": "dev-enhancements",
    "schmunk42/yii2-giiant": "0.8.*",
