    "kartik-v/yii2-mpdf": "1.0.*",
    "kartik-v/yii2-password": "dev-extended",
    "2amigos/yii2-file-upload-widget": "1.0.*",
    "2amigos/yii2-date-picker-widget": "1.0.*",
    "unclead/yii2-multiple-input": "1.4.*",
    "lavrentiev/yii2-toastr": "2.0.*",
    "2amigos/yii2-translateable-behavior": "*",
    "bower-asset/admin-lte": "2.3.*",
    "bower-asset/font-awesome": "4.5.*",
    "bower-asset/html5shiv": "3.7.*",
    "bower-asset/jquery-slimscroll": "1.3.*",
    "bower-asset/flot": "0.8.*",
    "bower-asset/respond": "1.4.*",
    "bower-asset/modernizr": "2.8.*",
    "bower-asset/nwmatcher": "1.3.*",
    "bower-asset/rem-unit-polyfill": "1.3.*",
    "bower-asset/selectivizr": "1.0.*",
    "bower-asset/jquery-placeholder": "2.0.*",
    "bower-asset/jquery.cookie": "1.4.*",
    "bower-asset/normalize.css": "3.0.*",
    "bower-asset/fastclick": "1.0.*",
    "bower-asset/flag-icon-css": "1.2.*",
    "bower-asset/foundation": "5.5.*",
    "bower-asset/foundation-datepicker": "1.5.*",
    "bower-asset/iframe-resizer": "3.5.*",
    "bower-asset/sweetalert": "*",
    "bower-asset/clipboard": "1.5.*",
    "bower-asset/spin.js": "2.3.*",
    "bower-asset/html5-history-api": "4.2.7.0",
    "bower-asset/ckeditor": "4.5.*",
    "skeeks/yii2-assets-auto-compress": "1.2.2",
    "bower-asset/blueimp-load-image": "2.6.2",
    "bower-asset/blueimp-canvas-to-blob": "3.3.0",
    "bower-asset/blueimp-tmpl": "3.4.0"
