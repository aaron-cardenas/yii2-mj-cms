  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mobilejazz-contrib/yii2-oauth2-server"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mobilejazz-contrib/yii2-export"
    }
  ]
